<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
	<metadata>
		<id>SpecFlow.Actions.Docker</id>
		<version>$version$</version>
		<authors>$author$</authors>
		<owners>$owner$</owners>
		<description>SpecFlow aims at bridging the communication gap between domain experts and developers by binding business readable behavior specifications to the underlying implementation. Our mission is to provide a pragmatic and frictionless approach to Acceptance Test Driven Development and Behavior Driven Development for .NET projects today."</description>
		<dependencies>
			<group targetFramework=".NETFramework4.7.1">
				<dependency id="SpecFlow.Actions.Configuration" version="$version$"/>
				<dependency id="Ductus.FluentDocker" version="2.10.45" />
				<dependency id="SpecFlow" version="3.9.58" />
			</group>
			<group targetFramework=".NETCoreApp2.1">
				<dependency id="SpecFlow.Actions.Configuration" version="$version$"/>
				<dependency id="Ductus.FluentDocker" version="2.10.45" />
				<dependency id="SpecFlow" version="3.9.58" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="bin\$config$\net471\SpecFlow.Actions.Docker.*" target="lib\net471"/>
		<file src="bin\$config$\netcoreapp2.1\SpecFlow.Actions.Docker.SpecFlowPlugin.dll" target="lib\netcoreapp2.1"/>
		<file src="bin\$config$\netcoreapp2.1\SpecFlow.Actions.Docker.SpecFlowPlugin.pdb" target="lib\netcoreapp2.1"/>

		<file src="$SolutionDir$..\LICENSE" target="LICENSE" />
		<file src="$SolutionDir$specflow-icon.png" target="images\" />
	</files>
</package>